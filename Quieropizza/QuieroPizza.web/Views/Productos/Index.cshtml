@model List<QuieroPizza.web.Models.ProductoModel>

@{
    ViewBag.Title = "Index";
}

<h2>Productos</h2>

@foreach (var producto in Model)
{

    //<p>@producto.Codigo @producto.Descripcion</p>

    <table>
        <tr>
            <td><strong>Codigo</strong></td>
            <td><strong>Descripcion</strong></td>
        </tr>
        <tr>
            <td>@producto.Codigo</td>
            <td>@producto.Descripcion</td>
        </tr>
    </table>

}

